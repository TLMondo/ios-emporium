# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'ecommerce-demo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ecommerce-demo
  
pod 'Cache'
pod 'CodableFirebase'           # Convert firebase data to codable
pod 'DTTextField'               # UITextField subclass for displaying
pod 'IBAnimatable'              # Expands UIKit
pod 'Instabug'                  # User Feedback
pod 'IQKeyboardManagerSwift'    # Auto display adjustment for UITextFields + Keyboard appearances
pod 'KeychainAccess'
pod 'Kingfisher'                # Image downloading & caching
pod 'MGSwipeTableCell'
pod 'NVActivityIndicatorView'
pod 'QRCodeReader.swift', '~> 8.2.0'
pod 'tealium-swift'


  target 'ecommerce-demoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ecommerce-demoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
